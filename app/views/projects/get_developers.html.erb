<div class="container">
  <div class="row pt-5">
    <div class="col-12">
      <h2>Add Developers </h2>
    </div>
    <div class="col-12">
      <%= form_tag add_developers_project_path(@project) do %>
        <div class="row">
          <% @developers.each do |developer| %>
            <div class="col-1">
              <span><%= check_box_tag "developer_ids[]", developer.id, false %><%= developer.employee_id %></span>
            </div>
          <% end %>
        </div>
        <%= submit_tag 'submit', class: "btn btn-success btn-width mt-3" %>
      <% end %>
    </div>
  </div>
</div>