<div class="container">
  <div class="row pt-5">
    <div class="col-12">
      <%= link_to "Developer Todos Status", root_path, class: "btn btn-success float-right mb-3" %>
    </div>
    <div class="col-12">
      <table class="table table-striped table-bordered" style="width:100%">
        <thead>
          <tr>
            <th>Status</th>
            <% @data.each do |data| %>
              <th><%= data["name"] %></th>
            <% end %>
          </tr>
        </thead>
        <%= render 'todos_status' %>